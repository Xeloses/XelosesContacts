    "todo-tree.general.exportPath": "./TODO.md",
    "todo-tree.general.showActivityBarBadge": false,
    "todo-tree.filtering.ignoreGitSubmodules": true,
    "todo-tree.tree.scanMode": "current file",
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.groupedByTag": true,
    "todo-tree.tree.buttons.filter": false,
    "todo-tree.tree.buttons.scanMode": true,

    "todo-tree.regex.regex": "(^\\s*)--\\s+(@|#)($TAGS)",

    "todo-tree.general.tags": [
        "DOC", "DOCS", "INFO",
        "LINK", "REF", "SEE",
        "TODO",
        "DEBUG", "TEST",
        "LOG", "LOGS",
        "BUG", "ERROR", "FIX", "FIXIT", "TMP",
        "SECTION",
    ],
    "todo-tree.general.tagGroups": {
        "SECTION": [ "SECTION" ],
        "BUG": [ "BUG", "ERROR", "FIX", "FIXIT", "TMP" ],
        "LOG": [ "LOG", "LOGS" ],
        "DEBUG": [ "DEBUG", "TEST" ],
        "TODO": [ "TODO" ],
        "DOC": [ "DOC", "DOCS", "INFO" ],
        "LINK": [ "LINK", "REF", "SEE" ],
    },

    "todo-tree.highlights.defaultHighlight": {
        "type": "whole-line",
        "fontWeight": "bold",
